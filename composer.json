{
  "name": "fm-labs/cakephp-cron",
  "description": "Cron Job plugin for CakePHP",
  "type": "cakephp-plugin",
    "license": "MIT",
  "require": {
    "php": ">=7.2",
    "cakephp/cakephp": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Cake\\": "./vendor/cakephp/cakephp",
      "Cron\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
      "Cron\\Test\\": "tests"
    }
  }
}
